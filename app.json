{
  "expo": {
    "name": "VarCal",
    "slug": "VarCal",
    "version": "0.1.1",
    "orientation": "portrait",
    "icon": "./assets/VarCalAppIcon.png",
    "splash": {
      "image": "./assets/VarCalAppIcon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "VarCal"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/VarCalAppIcon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.varcal",
      "versionCode": 2
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "description": "Calculator with variables"
  }
}
